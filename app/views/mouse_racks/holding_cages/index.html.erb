<p id="notice"><%= notice %></p>

<h1>Listing Holding Cages</h1>

<table>
  <thead>
    <tr>
      <th>Cage</th>
      <th>Occupancy</th>
      <th>Sex</th>
      <th>Dob</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @holding_cages.each do |holding_cage| %>
      <tr>
        <td><%= holding_cage.cage_id %></td>
        <td><%= holding_cage.occupancy %></td>
        <td><%= holding_cage.sex %></td>
        <td><%= holding_cage.dob %></td>
        <td><%= link_to 'Show', holding_cage %></td>
        <td><%= link_to 'Edit', edit_holding_cage_path(holding_cage) %></td>
        <td><%= link_to 'Destroy', holding_cage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Holding cage', new_holding_cage_path %>
